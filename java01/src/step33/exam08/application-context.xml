<?xml version="1.0" encoding="UTF-8"?>
<!-- beans 및 모든속성이 xmlns에 소속되어있다./// 이건 xml 스키마 -->
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

      
    <!--java.util.Date 타입의 프로퍼티 값 설정하기 -->
    
    <!-- String => java.util.Date으로 변환할수 없어, 예외발생! -->
    <bean id="c1" class="step33.exam08.Car">
      <property name="model" value="티코"/>
      <property name="maker" value="비트자동타"/>
      <property name="madeDate" value="2016-4-25"/>
    </bean>
    
    

</beans>